import string
import re

def isPalindrome(str):

    # Run loop from 0 to len/2
    for i in range(0, int(len(str)/2)):
     
    
        if str[i] != str[len(str)-i-1]:
            return False
    return True
str = "Madam, in Eden, I'm Adam"

s1=re.sub("[^A-Za-z]","",str) 

print(s1)
print (s1.lower())
ans = isPalindrome(s1)

if (ans):
    print("True")
else:
    print("False")
